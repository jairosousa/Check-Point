entity Controle {
	data LocalDate required unique,
    hrEntrada String required,
    hrAlmocoSaida String,
    hrAlmocoRetorno String,
    hrSaida String,
    corPulseira String, 
    observacao String
}
// Set pagination options
paginate Controle with pagination

// Use Data Transfert Objects (DTO)
dto * with mapstruct

// Set service options to all except few
service all with serviceImpl

